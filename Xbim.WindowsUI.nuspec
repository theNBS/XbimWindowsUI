			<dependency id="Xbim.Scripting" version="3.0.12-Prerelease" />
			<dependency id="HelixToolkit" version="2015.1.461" />
			<dependency id="PropertyTools.Wpf" version="2013.1.12.1" />
        </dependencies>
      </metadata>
